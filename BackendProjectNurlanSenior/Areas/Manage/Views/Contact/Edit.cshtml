@model Contact
@{
    ViewData["Title"] = "Create";
}

<div class="container mt-3">
    <h1 class="text text-success">Edit contact</h1>
    <div class="alert alert-danger" asp-validation-summary="ModelOnly"></div>

    <form method="post" class="mt-2" enctype="multipart/form-data">
      
       




        <div class="mb-2">
            <label asp-for="@Model.Address" class="form-label">Address</label>
            <input asp-for="@Model.Address" class="form-control">
            <span class="mt-2 text text-danger" asp-validation-for="@Model.Address"></span>
        </div>
        <div class="mb-2">
            <label asp-for="@Model.AdressLogo" class="form-label">Adress icon</label>
            <input asp-for="@Model.AdressLogo" class="form-control">
            <span class="mt-2 text text-danger" asp-validation-for="@Model.AdressLogo"></span>
        </div>
        <div class="mb-2">
            <label asp-for="@Model.Phone" class="form-label">Phone</label>
            <input asp-for="@Model.Phone" class="form-control">
            <span class="mt-2 text text-danger" asp-validation-for="@Model.Phone"></span>
        </div>
        <div class="mb-2">
            <label asp-for="@Model.PhoneLogo" class="form-label">Phone icon</label>
            <input asp-for="@Model.PhoneLogo" class="form-control">
            <span class="mt-2 text text-danger" asp-validation-for="@Model.PhoneLogo"></span>
        </div>
        <div class="mb-2">
            <label asp-for="@Model.Country" class="form-label">Country</label>
            <input asp-for="@Model.Country" class="form-control">
            <span class="mt-2 text text-danger" asp-validation-for="@Model.Country"></span>
        </div>
        <div class="mb-2">
            <label asp-for="@Model.CountryLogo" class="form-label">Country icon</label>
            <input asp-for="@Model.CountryLogo" class="form-control">
            <span class="mt-2 text text-danger" asp-validation-for="@Model.CountryLogo"></span>
        </div>
        <div class="mb-2">
            <label asp-for="@Model.LeaveReply" class="form-label">LeaveReply</label>
            <input asp-for="@Model.LeaveReply" class="form-control">
            <span class="mt-2 text text-danger" asp-validation-for="@Model.LeaveReply"></span>
        </div>


        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>

@section Scripts{
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.3/jquery.validate.min.js" integrity="sha512-37T7leoNS06R80c8Ulq7cdCDU5MNQBwlYoy1TX/WUsLFC2eYNqtKlV0QjH7r8JpG/S0GUMZwebnVFLPd6SU5yg==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/3.2.12/jquery.validate.unobtrusive.min.js" integrity="sha512-o6XqxgrUsKmchwy9G5VRNWSSxTS4Urr4loO6/0hYdpWmFUfHqGzawGxeQGMDqYzxjY9sbktPbNlkIQJWagVZQg==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
}



